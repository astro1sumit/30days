import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int number[]=new int[n];
		for(int i=0;i<n;i++){
		    number[i]=sc.nextInt();
		}
		for(int i=0;i<n;i++){
		    int count=number[i];
		    if((count=count+1)!=number[i+1]){
		        System.out.println(count++);
		        System.exit(0);
		    }
		}
	}
}
