import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    StringBuilder str = new StringBuilder("welcome to LinkedIn");
	    
	    for(int i=0; i<str.length()/2; i++)
	    {
	        int f = i;
	        int l = str.length() - i - 1;
	        
	        char ftc = str.charAt(f);
	        char ltc = str.charAt(l);
	        
	        str.setCharAt(f, ltc);
	        str.setCharAt(l, ftc);
	    }
	    System.out.println(str);
	}
}
