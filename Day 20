import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		if(n<=0){
		    System.out.print("false");
		}
		while(n>0){
		if(n==1)
		{
		    System.out.print("true");
		    System.exit(0);
		}
		else if(n%2==0)
		{
		    n=n/2;
		}
		else
		{
		    System.out.print("false");
		    System.exit(0);
		}
		}
	}
}
