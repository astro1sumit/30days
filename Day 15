import java.util.*;
public class Main 
{
    public static void main(String[] args){
        Scanner in=new Scanner(System.in);
        String str=in.next();
        for(int i=0;i<str.length();i++){
            if(str.indexOf(str.charAt(i),str.indexOf(str.charAt(i))+1)==-1 ){
                System.out.println(str.indexOf(str.charAt(i))+1);
                break;
            }
        }
    }

}
