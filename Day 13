import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner in=new Scanner(System.in);
	    String str1=in.next();
	    String str2=in.next();
	    if(str1.length()==str2.length()&&((str1+str1).indexOf(str2)!=-1)){
	        System.out.println("true");
	    }
	    else{
	        System.out.println("false");
	    }
	}
}
